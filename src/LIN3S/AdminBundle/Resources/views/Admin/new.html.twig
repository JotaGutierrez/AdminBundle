{% extends '@LIN3SAdmin/layout.html.twig' %}

{% form_theme form '@LIN3SAdmin/form.html.twig' %}

{% block lin3s_admin_title %}
    {% if entityConfig.printName is defined %}
        {% set entity = entityConfig.printName | lower %}
    {% else %}
        {% set entity = entityConfig.name %}
    {% endif %}
    <h1>
        {% trans with {'%entity%': entity} %}
            lin3s_admin.new.title
        {% endtrans %}
    </h1>
{% endblock %}

{% block lin3s_admin_content %}
    {{ form(form) }}
{% endblock %}
